{
    "name": "youri/parser-shacl",
    "description": "SHACL parser extending parser-rdf with SHACL-specific extraction.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Youri van den Bogert"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "youri/parser-rdf": "dev-main"
    },
    "require-dev": {
        "pestphp/pest": "^3.8",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Youri\\vandenBogert\\Software\\ParserShacl\\": "src/"
        },
        "files": ["aliases.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ParserShacl\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest --colors=always --coverage-clover=coverage.xml",
        "analyse": "vendor/bin/phpstan analyse --no-progress"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}

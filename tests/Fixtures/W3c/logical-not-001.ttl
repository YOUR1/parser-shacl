@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ex: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:NotStringShape
    a sh:NodeShape ;
    sh:targetClass ex:Resource ;
    sh:property [
        sh:path ex:value ;
        sh:not [
            sh:datatype xsd:string ;
        ] ;
    ] .
